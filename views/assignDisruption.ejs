<body class="noneMain">
	<ul>
	          <li class= "left"><a class="logo">Centralis</a></li>
	          <li class="dropdown" style="float:right; margin-top:5px;">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-bars fa-2x"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-user">
                        <li><a href="/admin"> Home</a>
                        </li>
                        <li><a href="/createExercise"> Create Exercise</a>
                        </li>
                        <li><a href="/selectExercise"> Select Exercise</a>
                        </li>
                    </ul>
                    <!-- /.dropdown-user -->
                </li>
	</ul>
	<h1 style="text-align: center; margin-top: 50px;"><strong><span class="typing"></span></strong></h1>
		  <script >
		              $(function(){
		                  $(".typing").typed({
		                    strings: ["Assign scenario based on responses"],
		                    typeSpeed: 100
		                  });
		              });
        		 </script>
        	<div class = "formNoBorder" action="/deleteExercise" method="post">
		<div class = "wrapper">
			<form action="/submitResults" method="post">
			<%
				for (var ii = 0; ii < allRooms.length; ii++) {
					%>
						<p>Room #<%= allRooms[ii] %>: </p>
						<!-- <ul class="dropdown-menu"> -->
						<select name="<%=allRooms[ii]%>">
						<%
							for (var zz = 0; zz < scenarioChoices.length; zz++) {
						%>
							<option value="<%= scenarioChoices[zz].id %>">
								<%= scenarioChoices[zz].name %>
							</option>
							<br/>
						<% 
							}
						%>
						</select>
					<!-- </ul> -->
					<%
				}
			%>
				<br/> </br/> <br/>
				<input type="hidden" name="sessionID" value="<%= sessionID %>"/>
				<button type="submit" class="btn btnNew">Send To Rooms</button>
			</form>
		</div>
			
		
	</div>


</body>
</html>
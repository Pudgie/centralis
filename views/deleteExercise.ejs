	<title>Delete Exercise</title> 
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
	 <link href="/bootstrap/css/centralis.css" rel="stylesheet" >
 	 <script src="bootstrap/js/jquery.min.js"></script>
  	<script src = "/bootstrap/js/clean-blog.js"></script>
  	<!-- javascript -->
 	 <script src="/bootstrap/js/bootstrap.min.js"></script>
  	<script src="/bootstrap/js/typed.js"></script>


  
</head>

<body class="noneMain">
	<ul>
	          <li class= "left"><a class="logo">Centralis</a></li>
	          <li class="dropdown" style="float:right; margin-top:5px;">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-bars fa-2x"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-user">
                        <li><a href="/admin"> Home</a>
                        </li>
                        <li><a href="/createExercise"> Create Exercise</a>
                        </li>
                        <li><a href="/selectExercise"> Select Exercise</a>
                        </li>
                    </ul>
                    <!-- /.dropdown-user -->
                </li>
	</ul>
	<h1 style="text-align: center; margin-top: 50px;"><strong><span class="typing"></span></strong></h1>
		  <script >
		              $(function(){
		                  $(".typing").typed({
		                    strings: ["Select the exercises you would like to delete: "],
		                    typeSpeed: 50
		                  });
		              });
        		 </script>
	<div class = "formNoBorder" action="/deleteExercise" method="post">
		<div class = "wrapper">
			
				<%
					for (var ii = 0; ii < exercises.length; ii++) {
						%>
							   <div class="funkyradio">
							        <div class="funkyradio-warning">
							            <input type="checkbox" name="exerciseChecks"   id="checkbox<%=ii+1%>"
							            value = "<%= exercises[ii]._id %>"/>
							            <label for="checkbox<%=ii+1%>"><%= exercises[ii].title %></label>
							        </div>
							    </div>

							
						<%
					}
				%>
			
		</div>
		<button id="select" type="submit" class="btn btnNew" value="Select">Select</button>
		<div class="divider"></div>
		<button type="button" class="btn btnNew" onclick="location.href = '/admin'">Go Back</button>
	</div>



</body>
</html>